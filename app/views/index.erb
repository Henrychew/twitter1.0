<h2>stalk here</h2>

<form action="/" method='post'>
  <label>Username</label>
  <input name='username'>
  <input type="submit" value="Submit">
</form>


<% if @tweets%>
  <div class="container">
    <h2>The tweets</h2>
    <ul>
      <% @tweets.each do |tweet| %>
        <li><%= tweet.text %></li>
      <%end%>
    </ul>
  </div>

<%end%>